<Window x:Class="AE2.Tools.Views.EmulatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:AE2.Tools.Views"
        Title="EmulatorWindow" Height="380" Width="440" Closed="Window_Closed">
    <DockPanel>
        <ContentControl Content="{Binding Control}" Width="240" Height="320" DockPanel.Dock="Left"/>
        <local:KeyboardControl Background="LightGray"/>
    </DockPanel>
</Window>
